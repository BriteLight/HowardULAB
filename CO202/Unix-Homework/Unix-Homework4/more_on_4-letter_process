[Brad@Computer2 ~/<7>Unix-Homework/Unix-Homework4]$ cat qua-word.sh 
#!/bin/bash

for word in $(man look | sed 's/,/ /g');
do
	if [ `echo $word | wc -c` == 5 ]; then
		echo $word >> 4_letter-words_file
		echo $word;
	fi
done
